{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended", "tslint-eslint-rules", "tslint-config-prettier"],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "member-access": false,
        "no-console": [false, "log"],
        "interface-name": [true, "never-prefix"]
    },
    "rulesDirectory": []
}
